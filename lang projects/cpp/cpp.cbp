<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="cpp" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/cpp" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-g" />
					<Add option="-DBUILD_DLL" />
				</Compiler>
				<Linker>
					<Add library="Nullscript-d" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="bin/cpp" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O3" />
					<Add option="-Wall" />
					<Add option="-DBUILD_DLL" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="Nullscript" />
					<Add library="xml" />
				</Linker>
				<ExtraCommands>
					<Add after='cmd /c copy &quot;$(PROJECT_DIR)$(TARGET_OUTPUT_FILE)&quot; &quot;$(PROJECT_DIR)/../../bin/Release/langs/cpp.dll&quot;' />
				</ExtraCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-std=c++11" />
			<Add directory="../../include" />
		</Compiler>
		<Linker>
			<Add option="--static" />
		</Linker>
		<Unit filename="main.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
